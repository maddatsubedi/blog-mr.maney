---
const { profile_data } = Astro.props;
---

<section
  class="number relative overflow-hidden md:h-72 py-5 flex justify-center"
>
  <div
    class="wrapper flex h-full items-center justify-around gap-5 flex-wrap md:px-0 px-4"
  >
    {
      profile_data.map((data) => {
        <div class="card-s rounded-md h-24 min-w-52 flex flex-col items-center justify-center md:w-auto w-full">
          <div class="text-white text-3xl">{data.label}</div>
          <div class="text-white">{data.text}</div>
        </div>;
      })
    }
  </div>
</section>
